{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/"],
  "entryPointStrategy": "expand",
  "tsconfig": "./tsconfig.json",
  "out": "./docs/code",
  "plugin": ["typedoc-theme-hierarchy"],
  "theme": "hierarchy",
  "name": "Node TypeScript Rest API",
  "includeVersion": true,
  "readme": "./docs/code.md",
  "exclude": [
    "./src/tests/*",
    "./src/middlewares/*.*.spec.ts",
    "./src/resources/**/*.*.spec.ts",
    "./src/resources/**/*.*.test.ts"
  ]
}